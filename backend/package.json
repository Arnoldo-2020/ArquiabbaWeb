    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.37.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0"
  }
}